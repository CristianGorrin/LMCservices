                                <Grid.RowDefinitions>
                                    <RowDefinition Height="25" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="100" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="100" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>

                                <StackPanel HorizontalAlignment="Left" Width="100" VerticalAlignment="Top" Grid.Row="1">
                                    <Label Content="Kunde nr." HorizontalAlignment="Right"/>
                                    <Label Content="Fornavn" HorizontalAlignment="Right" Margin="0,5,0,0"/>
                                    <Label Content="Efternavn" HorizontalAlignment="Right" Margin="0,5,0,0"/>
                                    <Label Content="Addresse" HorizontalAlignment="Right" Margin="0,5,0,0"/>
                                    <Label Content="Postnr. / by" HorizontalAlignment="Right" Margin="0,5,0,0"/>
                                    <Label Content="Tlf.nr." HorizontalAlignment="Right" Margin="0,5,0,0"/>
                                    <Label Content="Tlf.nr.2" HorizontalAlignment="Right" Margin="0,5,0,0"/>
                                    <Label Content="E-mail" HorizontalAlignment="Right" Margin="0,5,0,0"/>
